{
	"manifest_version": 2,

	"name": "Steam Winter Sale 2015",
	"description": "Automaticall skips through discovery queue",
	"version": "0.1",

	"content_scripts": [
		{
			 "matches": ["https://store.steampowered.com/agecheck/*"],
			 "js": ["agecheck.js"]
		},{
			 "matches": ["https://store.steampowered.com/app/*/agecheck*"],
			 "js": ["appagecheck.js"]
		},{
			 "matches": ["https://store.steampowered.com/*"],
			 "js": ["main.js"]
		}
	]
}
